{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/|.*\\.).*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/send-email.js": {
            "maxDuration": 10
        }
    }
}